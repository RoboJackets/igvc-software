<launch>
    <node pkg="igvc_perception" type="pointcloud_filter" name="pointcloud_filter_node" output="screen">
        <rosparam command="load" file="$(find igvc_perception)/config/pointcloud_filter.yaml" />
    </node>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find igvc_perception)/rviz/pt_filter.rviz"/>
    <!--Add your bag file path here-->
    <node pkg="rosbag" type="play" name="play" output="screen" args="-q $(find igvc_perception)/bag/JESSII_RUN_SUN_3_2019-06-10-16-06-21.bag"/>
</launch>
